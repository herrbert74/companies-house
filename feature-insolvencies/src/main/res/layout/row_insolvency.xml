<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
			  xmlns:tools="http://schemas.android.com/tools"
			  android:layout_width="match_parent"
			  android:layout_height="wrap_content"
			  android:gravity="center"
			  android:orientation="vertical"
			  android:paddingBottom="@dimen/viewMargin">

	<TextView
		android:id="@+id/lblInsolvencyDate"
		style="@style/Text.Bold"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="start"
		tools:text="2016-01-31"/>

	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal">

		<TextView
			android:id="@+id/lblInsolvencyType"
			style="@style/Text"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="1"
			tools:text="12341234"/>

		<TextView
			android:id="@+id/lblInsolvencyNumber"
			style="@style/Text"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:gravity="center"
			tools:text="Satisfied"/>
	</LinearLayout>

	<TextView
		android:id="@+id/lblInsolvencyPractitioner"
		style="@style/Text"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_gravity="start"
		tools:text="Ameuri Limited"/>

</LinearLayout>