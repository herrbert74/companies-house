<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="AppTheme">
		<attr name="singleLineStyle" format="reference" />
		<attr name="twoLineStyle" format="reference" />
	</declare-styleable>
	<declare-styleable name="SingleLineView">
		<attr name="imageViewSrc" format="reference" />
		<attr name="imageViewContentDescription" format="string" />
		<attr name="textSize" format="dimension" />
		<attr name="text" format="string" />
	</declare-styleable>
	<declare-styleable name="TwoLineView">
		<attr name="textSizeFirst" format="dimension" />
		<attr name="textFirst" format="string" />
		<attr name="textSizeSecond" format="dimension" />
		<attr name="textSecond" format="string" />
		<attr name="textSecondMaxLines" format="string" />
	</declare-styleable>
	<declare-styleable name="AddressView">
		<attr name="title" format="string" />
		<attr name="addressLine1" format="string" />
		<attr name="addressLine2" format="string" />
		<attr name="postCode" format="string" />
		<attr name="locality" format="string" />
		<attr name="region" format="string" />
		<attr name="country" format="string" />
	</declare-styleable>
</resources>
